<?php
$num_students = (int)readline("Enter the number of students: ");

for ($i = 1; $i <= $num_students; $i++) {
    $marks = (float)readline("Enter marks for student $i: ");

    if ($marks >= 90) {
        $grade = 'A';
    } elseif ($marks >= 80) {
        $grade = 'B';
    } elseif ($marks >= 70) {
        $grade = 'C';
    } elseif ($marks >= 60) {
        $grade = 'D';
    } else {
        $grade = 'F';
    }

    echo "Student $i Grade: $grade\n";
}
?>
