<?php
$a = 30;
$b = 66;

echo "Addition: " . ($a + $b) . "\n";         
echo "Subtraction: " . ($a - $b) . "\n";   
echo "Multiplication: " . ($a * $b) . "\n";  
echo "Division: " . ($a / $b) . "\n";         
echo "Modulus: " . ($a % $b) . "\n";          
echo "Exponent: " . ($a ** $b) . "\n";        
?>
