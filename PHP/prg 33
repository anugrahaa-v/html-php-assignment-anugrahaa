<?php
// Create a 3x3 matrix (multidimensional array)
$matrix = array(
    array(1, 2, 3),       // Row 0
    array(4, 5, 6),       // Row 1
    array(7, 8, 9)        // Row 2
);

// Access and display the value in the second row (index 1) and third column (index 2)
echo "Value at second row, third column: " . $matrix[1][2];
?>
